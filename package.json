{
  "private": true,
  "workspaces": {
    "packages": [
      "api",
      "web",
      "packages/*"
    ]
  },
  "devDependencies": {
    "@redwoodjs/core": "3.3.0",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13"
  },
  "eslintConfig": {
    "extends": "@redwoodjs/eslint-config",
    "root": true,
    "rules": {
      "@typescript-eslint/ban-ts-comment": "off",
      "react-hooks/exhaustive-deps": "off"
    }
  },
  "engines": {
    "node": ">=14.19 <=16.x",
    "yarn": ">=1.15"
  },
  "prisma": {
    "seed": "yarn rw exec seed"
  },
  "packageManager": "yarn@3.2.3"
}
