{
  "github": {
    "silent": true
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [{ "key": "Access-Control-Allow-Origin", "value": "*" }]
    }
  ],
  "rewrites": [
    {
      "source": "/customizer",
      "destination": "https://scrapbook-customizer.vercel.app/"
    },
    {
      "source": "/customizer/(.*)",
      "destination": "https://scrapbook-customizer.vercel.app/$1"
    }
  ]
}
